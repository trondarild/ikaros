<?xml version="1.0"?>

<group title="Example">
	<!-- Modules -->
		<module
			class="SimpleListIterator"
			name="ListIt"
			repeat = "yes"
			list = "0 45 90"
			list_length = "3"
			period = "10"
		/>
	
	<!--Connections-->	

	<!-- View -->
		<view>
			<object class="Plot " 
				module="ListIt" 
				source="OUTPUT" 
				x="0" y="0" w="2" h="2"
				min="0"
				max="90"
			/>
		</view>

</group>