<?xml version="1.0"?>

<group title="ListIterator Example">
		<module
			class = "Constant"
			name = "Index"
			data = "1"
		/>

		<module
        class       =   "FunctionGenerator"
        name        =   "SquareGen"
        type        =   "ticksquare"
        _frequency  =  "0.001"
        basetime		= 	"30"	
        offset      =   "0.0"
        amplitude   =   "1"
        shift       =   "0.0"
        tickduty	  = 	"1"
   	/>

		<module
			class="ListIterator"
			name="ListIterator"
			repeat = "true"
			list = "1"
			list_length = "1"
			debug = "true"
		/>
		
        <connection sourcemodule = "Index" source = "OUTPUT" targetmodule = "ListIterator" target = "SELECT" />
        <connection sourcemodule = "SquareGen" source = "OUTPUT" targetmodule = "ListIterator" target = "SYNC IN" />

        
		<view name="View">
			<object class="Plot " 
				module="SquareGen" 
				source="OUTPUT" 
				x="0" y="0" w="1" h="1"
				min="0"
				max="2"
			/>
			<object class="Plot " 
				module="ListIterator" 
				source="OUTPUT" 
				x="0" y="1" w="1" h="1"
				min="0"
				max="4"
			/>
			<object class="Plot " 
				module="ListIterator" 
				source="SYNC OUT" 
				x="0" y="2" w="1" h="1"
				min="0"
				max="2"
			/>  
		</view>

</group>
 
