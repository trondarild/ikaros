<?xml version="1.0"?>

<group name="Product" description="multiplies all input">

	<description type="text">
		Module that multiplies all elements in its input.
	</description>

	<example description="A simple example">
		<module
			class="Product"
			name="Product"
		/>
	</example>

	<input name="INPUT" description="The  input" />

	<output name="OUTPUT" description="The Product" size="1" />

	<module class="Product" />

    <view title="Product">
        <object
            x="0" y="0" w="2"
            class="BarGraph"
            module="*"
            source="OUTPUT"
            title="Default view for Product"
        />
    </view>
    
