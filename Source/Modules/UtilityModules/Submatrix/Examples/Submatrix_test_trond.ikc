<?xml version="1.0"?>

<group>
	
		<module class = "Constant" name = "Constant" data = "1 2 3; 2 3 4; 3 4 5" />
	
		<module
			class = "Submatrix"
			name = "Submatrix"
            x0 = "0"
            x1 = "3"
            y0 = "1"
            y1 = "3"
		/>

		<connection sourcemodule="Constant" source="OUTPUT" targetmodule="Submatrix" target="INPUT" />

		<view name="View">
            <object
            class="Table"
            module="Submatrix"
            source="OUTPUT"
            x="0"
            y="0" w="2" h="2"
            />

		</view>

</group>
 
