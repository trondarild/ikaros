<?xml version="1.0"?>

<group>
	
		<module
			class = "Randomizer"
			name = "R"
			outputsize = "10"
			min = "-1"
			max = "1"
             distribution = "gaussian"
             meanval = "0"
             variance = "0.5"
            
		/>



		<view name="V1">
			<object class="BarGraph" module="R" source="OUTPUT" x="0" y="0" min="-2" max="2" negative_fill="red"/>
		</view>
</group>
 
